{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-json": "*",
        "anandsiddharth/laravel-paytm-wallet": "^2.0",
        "anhskohbo/no-captcha": "^3.0",
        "barryvdh/laravel-dompdf": "^3.0",
        "baselrabia/myfatoorah-with-laravel": "^1.0",
        "cartalyst/stripe-laravel": "^16.0",
        "doctrine/dbal": "^4.0",
        "guzzlehttp/guzzle": "^7.0",
        "instamojo/instamojo-php": "^0.4",
        "ixudra/curl": "^6.22",
        "iyzico/iyzipay-php": "^2.0",
        "laravel-lang/lang": "15.0",
        "laravel-notification-channels/webpush": "^10.0",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "masterro/laravel-xss-filter": "^2.0",
        "mews/purifier": "^3.3",
        "midtrans/midtrans-php": "^2.6",
        "mollie/laravel-mollie": "^3.0",
        "phpmailer/phpmailer": "^6.1",
        "rachidlaasri/laravel-installer": "^4.1",
        "razorpay/razorpay": "2.*",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-sitemap": "^7.0",
        "unicodeveloper/laravel-paystack": "^1.0",
        "unisharp/laravel-filemanager": "^2.2",
        "willvincent/feeds": "^2.1"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^12.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": ["app/Http/Helpers/Helper.php", "app/Http/Helpers/Sections.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
